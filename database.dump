-- Name: recherche(text); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION public.recherche(text text) RETURNS TABLE(modele text, categorie text, prix real, nom_vendeur text, prenom_vendeur text)
    LANGUAGE sql
    AS $$
    SELECT article.modele, categorie.nom, article.prix, utilisateur.nom, utilisateur.prenom  FROM article, utilisateur, categorie
    WHERE article.vendeur=utilisateur.id
	AND article.categorie=categorie.id
	AND (
	      LOWER(article.description) LIKE LOWER(text)
       OR LOWER(article.marque) LIKE LOWER(text)
       OR LOWER(article.serie) LIKE LOWER(text)
       OR LOWER(article.modele) LIKE LOWER(text)
	)
$$;


